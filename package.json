{
  "name": "drum-machine",
  "version": "1.6.0",
  "description": "drum machine",
  "main": "src/js/main.js",
  "scripts": {
    "start": "npm run build-css && react-scripts start",
    "build": "CI=false; npm run build-css && react-scripts build",
    "build-css": "less-watch-compiler --run-once ./src/less/ ./src/less/ all.less",
    "watch-css": "less-watch-compiler ./src/less/ ./src/less/ all.less",
    "test": "CI=true; react-scripts test --env=jsdom --coverage",
    "test-watch": "CI=false; react-scripts test --env=jsdom --watchman",
    "coverage": "react-scripts test --env=jsdom --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://matt_bridgeman@bitbucket.org/matt_bridgeman/drum-machine.git"
  },
  "devDependencies": {
    "@alrra/travis-scripts": "^3.0.1",
    "chai": "^4.2.0",
    "less": "^3.9.0",
    "less-watch-compiler": "^1.13.0",
    "moment": "^2.24.0",
    "react-scripts": "^2.1.8",
    "rxjs": "^6.3.3",
    "testdouble": "^3.11.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.3.2",
    "firebase": "^5.9.0",
    "firebaseui": "^3.5.2",
    "history": "^4.9.0",
    "immutable": "^3.8.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
